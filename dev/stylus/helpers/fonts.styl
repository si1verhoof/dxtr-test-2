@font-face {
  font-family: 'GaramondBookNarrowC';
  src: url('fonts/GaramondBookNarrowC.woff2') format('woff2'),
    url('fonts/GaramondBookNarrowC.woff') format('woff');
  font-weight: 400;
  font-style: normal;
}

@font-face {
  font-family: 'FontAwesome';
  src: url('fonts/FontAwesome.woff2') format('woff2'),
    url('fonts/FontAwesome.woff') format('woff');
  font-weight: bold;
  font-style: italic;
}

